{
    "name": "ilicmiljan/retry-master",
    "description": "RetryMaster is a flexible and extensible PHP library for handling operation retries.",
    "keywords": ["retry-intervals", "retry-library", "retry-strategies", "retry-requests", "retry-pattern", "retry-policies"],
    "type": "library",
    "autoload": {
        "psr-4": {
            "IlicMiljan\\RetryMaster\\": "src/"
        },
        "exclude-from-classmap": [
            "tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IlicMiljan\\RetryMaster\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Miljan Ilic",
            "email": "hello@miljanilic.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "phpunit/phpunit": "^9.6",
        "phpstan/phpstan": "^1.10",
        "infection/infection": "^0.26.19"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
